<footer class="footer-area bg-image" style="background: #131323">
    <div class="copyright-area">
        <div class="container">
            <div class="row" style="padding-bottom: 30px; justify-content: space-between">
                <img src="/img/logo-footer.png" width="170px" height="55px">

                <div>
                    <img class="cursor" onclick="alert('Coming soon...')" src="/img/icon-reddit.png" width="41px"
                         height="41px">
                    <img class="cursor" onclick="alert('Coming soon...')" src="/img/icon-twitter.png" width="41px"
                         height="41px" style="margin: 0 10px 0">
                    <img class="cursor" onclick="alert('Coming soon...')" src="/img/icon-facebook.png" width="41px"
                         height="41px" style="margin: 0 10px 0 0">
                    <img class="cursor" onclick="alert('Coming soon...')" src="/img/icon-medium.png" width="41px"
                         height="41px">
                </div>
            </div>

            <div class="h-100 justify-content-center align-items-center"
                 style="display: flex; border-top: 1px solid #71718d; padding: 30px 0 50px">
                <p style="color: white !important">Copyright 2021. DAIB, Inc. all right reserved.</p>
            </div>
        </div>
    </div>
</footer>
